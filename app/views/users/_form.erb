<%= form_for(User.new) do |f| %>
    <%= f.label :first_name, 'First Name'%>
    <%= f.text_field :first_name%>
    <%= f.label :last_name, 'Last Name'%>
    <%= f.text_field :last_name%>
    <%= f.label :email, 'Email'%>
    <%= f.text_field :email%>
    <%= f.label :password, 'Password'%>
    <%= f.text_field :password%>
    <%= f.submit %>
<% end %>