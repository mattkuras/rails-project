<%if !com_params?%>
<%= form_for(Post.new) do |f| %>
    <%= f.label :content, 'Whats on your mind?'%>
    <%= f.text_area :content%>
    <%= f.submit %>
<% end %>

<%else%>

<%= form_for @post do |f| %>
<%byebug%>
    <%= f.label :content, 'Whats on your mind?'%>
    <%= f.text_area :content%>
    <%= f.submit %>
<% end %>
<% end %>